
<!DOCTYPE html>
<html lang="en">

<head>
    <title>Colo Shop</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Colo Shop Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="~/Assets/shop/styles/bootstrap4/bootstrap.min.css">
    <link href="~/Assets/shop/plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="~/Assets/shop/plugins/OwlCarousel2-2.2.1/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="~/Assets/shop/plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
    <link rel="stylesheet" type="text/css" href="~/Assets/shop/plugins/OwlCarousel2-2.2.1/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Assets/shop/styles/main_styles.css">
    <link rel="stylesheet" type="text/css" href="~/Assets/shop/styles/responsive.css">
    <link href="~/Assets/shop/styles/responsive.css" rel="stylesheet" />
    <link href="~/Assets/shop/styles/single_responsive.css" rel="stylesheet" />
    <link href="~/Assets/shop/styles/single_styles.css" rel="stylesheet" />
    <link rel=”stylesheet” href=”https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css”>
</head>

<body>

    <div class="super_container">

        <!-- Header -->

        <header class="header trans_300">
            <!-- Top Navigation -->

       @{ Html.RenderAction("TopMenu", "Home");}
            <!-- Main Navigation -->

            <div class="main_nav_container">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 text-right">
                            <div class="logo_container">
                                <a href="index.html">colo<span>shop</span></a>
                            </div>
                            <nav class="navbar">
                                @{Html.RenderAction("MainMenu", "Home");}
                                <ul class="navbar_user">
                                    @{ Html.RenderAction("HeaderCart", "Home");}
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <div class="fs_menu_overlay"></div>

        @RenderBody()

        <!-- Footer -->

        <footer class="footer">
            @{ Html.RenderAction("Footer", "Home");}
        </footer>
    </div>


    <script src="~/Assets/shop/js/jquery-3.2.1.min.js"></script>
    <script src="~/Assets/shop/styles/bootstrap4/popper.js"></script>
    <script src="~/Assets/shop/styles/bootstrap4/bootstrap.min.js"></script>


    <script src="~/Assets/shop/plugins/Isotope/isotope.pkgd.min.js"></script>
    <script src="~/Assets/shop/plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
    <script src="~/Assets/shop/plugins/easing/easing.js"></script>
    <script src="~/Assets/shop/js/custom.js"></script>

    <script src="~/Assets/shop/js/single_custom.js"></script>
    @RenderSection("jsFooter", required:false)

</body>

</html>